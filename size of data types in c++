#include<iostream>
using namespace std;
int main()
{
	int intType;
	float floatType;
	double doubleType;
	char charType;
	cout<<"Size of int is:"<<sizeof(intType)<<"bytes"<<endl;
	cout<<"Size of float is:"<<sizeof(floatType)<<"bytes"<<endl;
	cout<<"Size of double is:"<<sizeof(doubleType)<<"bytes"<<endl;
	cout<<"Size of char is"<<sizeof(charType)<<"bytes"<<endl;
	return 0;
}
